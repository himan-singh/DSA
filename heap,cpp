//heap data structure

#include<bits/c++.h>
using namespace std;

void insert(a[],n,value)
{
	n=n+1;
	a[n]=value;
	int i=n;
	while(i>1)
	{int parent=i/2;
	if(a[parent]>a[i])
	{
	swap(a,parent,i);
	i=parent;
	}
	
	else
	return 0;
	}


return 0; 
}

void delete(a[],n)
{
	a[1]=a[n];
	n-n-1;
	i=1;
	while(i<n)
	{
      int left=a[2*i];
      int right=a[2*i+1];
      int larger=left>right?2*i:2*i+1;
      if(a[i]>a[larger])
      {
      swap(a,i,larger)
      i=larger;
      }
      else
      return 0;

	}

	return 0;
}